<?xml version="1.0" encoding="utf-8"?>
<!-- Enable to change color during button click -->

<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:drawable="@color/button_clicked"
        android:state_pressed="true"
        ></item>
    <item android:drawable="@color/button_unclicked"></item>

</selector>
